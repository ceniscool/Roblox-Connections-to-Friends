{
  "manifest_version": 3,
  "name": "Roblox Friends Renamer",
  "version": "1.0",
  "description": "Replaces 'Connections' with 'Friends' on Roblox.",
  "content_scripts": [
    {
      "matches": ["*://*.roblox.com/*"],
      "js": ["replace.js"],
      "run_at": "document_idle"
    }
  ]
}
